{
	"schemaVersion": 1,
	"id": "clickvillagers",
	"version": "${mod_version}",
	"name": "ClickVillagers",
	"description": "Pick villagers up, change their biomes, claim them and much more!",
	"authors": [
		"Clickism"
	],
	"contact": {
		"homepage": "https://modrinth.com/plugin/clickvillagers",
		"sources": "https://github.com/Clickism/ClickVillagers"
	},
	"license": "ARR",
	"icon": "assets/clickvillagers/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"de.clickism.clickvillagers.platform.fabric.FabricEntrypoint"
		]
	},
	"mixins": [
		"clickvillagers.mixins.json"
